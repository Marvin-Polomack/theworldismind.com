{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/api/factcheck", "dest": "/api/factcheck.js" },
    { "src": "/api/status", "dest": "/api/status.js" },
    { "src": "/", "dest": "/api/status.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 