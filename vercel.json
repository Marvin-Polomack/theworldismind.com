{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "turbo build",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/api/factcheck/:path*", "destination": "/apps/TWIMChat-FactCheck/api/:path*" },
    { "source": "/(.*)", "destination": "/apps/TWIMChat/$1" }
  ]
} 